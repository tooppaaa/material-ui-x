{
  "name": "storybook",
  "version": "4.0.0-alpha.9",
  "description": "Storybook components",
  "author": "Material-UI Team",
  "private": true,
  "scripts": {
    "start": "start-storybook -p 6006 --ci",
    "build": "build-storybook -o dist/ -s storybook --quiet",
    "export": "mv dist ../../docs/export/storybook",
    "lint": "../../node_modules/.bin/tsc --noEmit && eslint 'src/**/*.{ts,tsx}' --quiet --fix -c ./.eslintrc.js",
    "test": "jest -c integration/setup/jest.config.js --detectOpenHandles --force-exit --runInBand",
    "test:ci": "start-server-and-test start http://localhost:6006 test",
    "typescript": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.12",
    "@material-ui/data-grid": "^4.0.0-alpha.9",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.54",
    "@material-ui/x-grid": "^4.0.0-alpha.9",
    "@material-ui/x-grid-data-generator": "^4.0.0-alpha.9",
    "@material-ui/x-license": "^4.0.0-alpha.9",
    "react": "^16.13.1",
    "react-is": "^16.13.1",
    "rxjs": "^6.5.5",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/addon-a11y": "^6.0.4",
    "@storybook/addon-actions": "^6.0.4",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-docs": "^6.0.4",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-knobs": "^6.0.4",
    "@storybook/addon-storysource": "^6.0.4",
    "@storybook/addon-viewport": "^6.0.4",
    "@storybook/react": "^6.0.4",
    "@storybook/theming": "^6.0.4",
    "@types/jest": "^26.0.0",
    "@types/jest-image-snapshot": "^4.1.0",
    "@types/puppeteer": "^5.4.0",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "jest": "^26.0.1",
    "jest-image-snapshot": "^4.1.0",
    "jest-puppeteer": "^4.4.0",
    "puppeteer": "^4.0.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "source-map-loader": "^1.0.2",
    "start-server-and-test": "^1.11.3",
    "string-replace-loader": "^2.3.0",
    "ts-loader": "^8.0.2",
    "typescript": "^4.1.5"
  }
}
